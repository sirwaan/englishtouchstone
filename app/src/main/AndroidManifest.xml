        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Units.U10"></activity>
        <activity android:name=".Units.U9" />
        <activity android:name=".Units.U8" />
