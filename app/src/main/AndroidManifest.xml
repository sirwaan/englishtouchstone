        <activity android:name=".Units.U5" />
        <activity android:name=".Units.U4" />
        <activity android:name=".Units.U3" />
        <activity android:name=".Units.U2" />
        <activity android:name=".Units.U1" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar.FullScreen" />
        <activity
            android:name=".Splash"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar.FullScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>